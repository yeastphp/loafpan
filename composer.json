{
  "name": "yeast/loafpan",
  "description": "Object and config hydration from associative arrays",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Yeast\\Loafpan\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yeast\\Test\\Loafpan\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "eater",
      "email": "=@eater.me"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "nette/php-generator": "^3.6",
    "brick/varexporter": "^0.3.5",
    "symfony/polyfill-php81": "^1.24",
    "jawira/case-converter": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.13",
    "ramsey/uuid": ">=4",
    "justinrainbow/json-schema": "^5.2",
    "phpstan/phpstan": "^1.4",
    "vimeo/psalm": "^4.19",
    "jetbrains/phpstorm-attributes": "^1.0"
  },
  "suggest": {
    "ramsey/uuid": "Allows parsing of UUID's in units"
  }
}
